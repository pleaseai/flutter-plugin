{
  "name": "flutter",
  "version": "0.3.0",
  "description": "Enables several Flutter and Dart-related commands and context.",
  "contextFileName": "flutter.md",
  "commands": ["./commands"],
  "mcpServers": {
    "dart": {
      "command": "dart",
      "args": [
        "mcp-server"
      ]
    }
  },
  "hooks": {
    "SessionStart": [
      {
        "matcher": "startup|resume",
        "hooks": [
          {
            "type": "command",
            "command": "${CLAUDE_PLUGIN_ROOT}/hooks/context.sh",
            "timeout": 10
          }
        ]
      }
    ]
  }
}
